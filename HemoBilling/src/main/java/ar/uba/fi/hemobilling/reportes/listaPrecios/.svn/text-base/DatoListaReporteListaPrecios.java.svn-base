package ar.uba.fi.hemobilling.reportes.listaPrecios;

/**
 * Esta clase contiene los campos de una fila del reporte Lista de PRecios
 * Notar que las columnas del JRXML de este reporte estan mapeadas a los atributos de esta clase
 * (ver createDynamicReport del ListaPrecios)
 * 
 * @author Ale
 *
 */
public class DatoListaReporteListaPrecios 
{
	private String codigo;
	private String denominacion;
	private String precio;
	
	public String getCodigo() {
		return codigo;
	}
	public void setCodigo(String codigo) {
		this.codigo = codigo;
	}
	public String getDenominacion() {
		return denominacion;
	}
	public void setDenominacion(String denominacion) {
		this.denominacion = denominacion;
	}
	public String getPrecio() {
		return precio;
	}
	public void setPrecio(String precio) {
		this.precio = precio;
	}
	
	

}
